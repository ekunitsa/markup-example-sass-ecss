<div class="gst-modelItem">
	<div class="gst-modelItem_inner">
		<div class="gst-modelItem_img">
			<picture>
				<source srcset="./img/model/img{{ loop.index }}.avif 1x, ./img/model/img{{ loop.index }}@2x.avif 2x" type="image/avif">
				<source srcset="./img/model/img{{ loop.index }}.webp 1x, ./img/model/img{{ loop.index }}@2x.webp 2x" type="image/webp">
				<img
                    class="gst-modelItem_imgSrc"
                    src="./img/model/img{{ loop.index }}.jpg"
                    alt="{{ item.title }}"
                    srcset="./img/model/img{{ loop.index }}.jpg 1x, ./img/model/img{{ loop.index }}@2x.jpg 2x"
                >
			</picture>
		</div>

		<div class="gst-modelItem_container">
			<div class="gst-modelItem_title">
				{{ item.title }}
			</div>

			<div class="gst-modelItem_text">
				{{ item.text }}
			</div>

			<div class="gst-modelItem_btnContainer">
				<a href="#callback" class="gst-modelItem_btn js-modal">
                    Get Catalog
                </a>
			</div>
		</div>
	</div>
</div>